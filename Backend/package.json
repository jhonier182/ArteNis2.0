{
  "name": "artenis-backend",
  "version": "1.0.0",
  "description": "Backend para ArteNis - App tipo Pinterest para tatuadores",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "prod": "cross-env NODE_ENV=production node src/server.js",
    "setup": "npm install",
    "test": "jest --runInBand --detectOpenHandles --coverage",
    "test:watch": "jest --watch",
    "test:feed": "jest --runInBand tests/feed.test.js",
    "test:controller": "jest --runInBand tests/postController.test.js",
    "test:integration": "jest --runInBand tests/integration",
    "lint": "eslint src/ tests/",
    "migrate": "node src/utils/migrate.js",
    "seed": "node src/utils/seed.js"
  },
  "keywords": [
    "tattoo",
    "pinterest",
    "api",
    "express",
    "mysql"
  ],
  "author": "ArteNis Team",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "memory-cache": "^0.2.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.1",
    "sharp": "^0.34.5",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
